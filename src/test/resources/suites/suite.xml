<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AdvantageSuite" guice-stage="DEVELOPMENT">
	<test name="Pruebas en Chrome" verbose="2">
		<parameter name="browser" value="chrome"/>
		<parameter name="url" value="https://www.advantageonlineshopping.com"/>
		<classes>
			<class name="test.CreateAccount"/>
			<class name="test.Login"/>
			<class name="test.Search"/>
			<class name="test.AddToCart"/>
			<class name="test.Checkout"/>
			<class name="test.Contact"/>
			<class name="test.Chat"/>
		</classes>
	</test>
