<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AdvantageSuite">
	<test name="Pruebas en Chrome" verbose="2">
		<parameter name="browser" value="chrome"/>
		<parameter name="hidden" value="false"/>
		<parameter name="url" value="https://www.advantageonlineshopping.com"/>
		<classes>
			<class name="legacy.advantage.tsd.Tsd_CreateAccount"/>
			<class name="legacy.advantage.tsd.Tsd_Login"/>
			<class name="legacy.advantage.tsd.Tsd_Search"/>
			<class name="legacy.advantage.tsd.Tsd_AddingToCart"/>
			<class name="legacy.advantage.tsd.Tsd_Checkout"/>
			<class name="legacy.advantage.tsd.Tsd_ContactUs"/>
			<class name="legacy.advantage.tsd.Tsd_Chat"/>
		</classes>
	</test>
</suite>